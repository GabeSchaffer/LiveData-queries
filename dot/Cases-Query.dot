    ORDER_CATALOG [label="ORDER_CATALOG"]
    PERSON [label="PERSON"]
    CASE_TIMES [label="CASE_TIMES"]
    CLINICAL_EVENT [label="CLINICAL_EVENT"]
    ENCNTR_ALIAS [label="ENCNTR_ALIAS"]
    MM_OMF_ITEM_MASTER [label="MM_OMF_ITEM_MASTER"]
